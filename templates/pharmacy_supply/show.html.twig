{% extends 'base.html.twig' %}

{% block title %}PharmacySupply{% endblock %}

{% block body %}
    <h1>PharmacySupply</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ pharmacy_supply.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ pharmacy_supply.Name }}</td>
            </tr>
            <tr>
                <th>International_Name</th>
                <td>{{ pharmacy_supply.InternationalName }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ pharmacy_supply.Description }}</td>
            </tr>
            <tr>
                <th>Quantity</th>
                <td>{{ pharmacy_supply.Quantity }}</td>
            </tr>
            <tr>
                <th>Exp_Date</th>
                <td>{{ pharmacy_supply.ExpDate ? pharmacy_supply.ExpDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Drug_List_N</th>
                <td>{{ pharmacy_supply.DrugListN ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_pharmacy_supply_index') }}">back to list</a>

    <a href="{{ path('app_pharmacy_supply_edit', {'id': pharmacy_supply.id}) }}">edit</a>

    {{ include('pharmacy_supply/_delete_form.html.twig') }}
{% endblock %}
